    "@types/multimap": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.1.1",
    "apidoc": "^1.2.0",
    "debug": "^4.3.5",
    "npm-watch": "^0.13.0",
    "shx": "^0.3.4",
    "supertest": "^7.0.0",
    "tplant": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^3.1.1"
  },
