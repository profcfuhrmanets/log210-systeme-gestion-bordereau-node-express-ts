{
  "name": "ets-log210-typescript-node-api",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "startWithDoc": "npm run compile && npm run all_docs && node dist/index.js",
    "start": "npm run compile && node dist/index.js",
    "start-docker": "apidoc -i src/routes/ -o dist/docs/ && node dist/index.js",
    "test": "jest --colors --silent",
    "watch": "jest --coverage --watchAll",
    "all_docs": "npm run docs && npm run madge && npm run tplant && npm run copydata",
    "docs": "apidoc -i src/routes/ -o dist/docs ",
    "circular": "madge src/App.ts -i dist/docs/madge.png -c",
    "madge": "madge src/App.ts -i dist/docs/madge.png",
    "tplant": "tplant --input src/App.ts  -A --output dist/docs/dcl.svg",
    "precompile": "npm run clean",
    "clean": "rimraf dist/controller && rimraf dist/model && rimraf dist/routes && rimraf dist/App.js && rimraf dist/index.js",
    "compile": "npm run compile:ts",
    "compile:ts": "tsc --outDir ./dist --module commonjs --downlevelIteration ./src/index.ts",
    "copydata": "cp -R src/data dist/data"
  },
  "watch": {
    "test": "{src,tests}/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.9",
    "@types/multimap": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "apidoc": "^1.2.0",
    "debug": "^4.3.5",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "madge": "^7.0.0",
    "npm-watch": "^0.13.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "tplant": "^3.1.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "multimap": "^1.1.0"
  }
}
